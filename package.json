{
  "name": "formipeek-qwik",
  "version": "0.0.1",
  "description": "FormiPeek - File Converter with Qwik + React",
  "type": "module",
  "scripts": {
    "dev": "vite dev --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js dev --mode ssr --force",
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/express/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run npm run qwik add'",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite preview --open",
    "serve": "node server/entry.express.js",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.18.0",
    "@builder.io/qwik-city": "^1.18.0",
    "@builder.io/qwik-react": "^0.5.3",
    "@types/eslint": "^8.56.10",
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-plugin-qwik": "^1.18.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "undici": "*",
    "vite": "^5.4.2",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "dependencies": {
    "@builder.io/qwik-react": "^0.5.3",
    "express": "^4.18.2",
    "i18next": "^23.7.0",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
